{
  "name": "mirrorize",
  "version": "0.1.0",
  "description": "A new framework for smart mirror/infoboard project",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirrorize/mirrorize.git"
  },
  "keywords": [
    "magicmirror",
    "smartmirror",
    "infoboard"
  ],
  "author": "mirrorize.net@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mirrorize/mirrorize/issues"
  },
  "homepage": "https://github.com/mirrorize/mirrorize#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/eslint-plugin": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.10.4",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "etag": "^1.8.1",
    "fastify": "^3.3.0",
    "fastify-cookie": "^4.0.2",
    "fastify-session": "^5.0.0",
    "fastify-static": "^3.2.0",
    "globby": "^11.0.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.8.12",
    "loglevel": "^1.7.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "mime": "^2.4.6",
    "uuid": "^8.3.0",
    "workbox-sw": "^5.1.3"
  },
  "engines": {
    "node": ">=14.9.0",
    "npm": ">=6.14.8"
  },
  "imports": {
    "#lib": "./lib/index.js"
  },
  "standard": {
    "parser": "@babel/eslint-parser"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-private-methods"
      ],
      [
        "@babel/plugin-proposal-private-property-in-object"
      ],
      ["@babel/plugin-proposal-class-properties"]
    ]
  }
}
